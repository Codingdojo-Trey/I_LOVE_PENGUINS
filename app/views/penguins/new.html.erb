<h1>MAKE A NEW PENGUIN!!!!</h1>
<p>Find me in app/views/penguins/new.html.erb</p>
<%= form_for(@penguin) do |f| %>
	<%= f.label :name, 'name'%>
	<%= f.text_field :name, :placeholder => 'this is name' %>
	<%= f.label :breed, 'breed' %>
	<%= f.text_field :breed %>
	<%= f.label :sex, 'sex' %>
	<%= f.text_field :sex %>
	<%= f.label :description, 'description' %>
	<%= f.text_area :description %>
	<%= f.submit 'make a penguiN!' %>
<% end %>
